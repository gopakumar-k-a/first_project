    <script>
        function addToWish(prId) {
            let size = document.getElementById('sizeSelect').value

            const requestData = {
                size: size,
                prId: prId
            };

            fetch('/add-to-wish', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json',
                },
                body: JSON.stringify(requestData)
            }).then(response => {
                if (response.ok) {
                    return response.json();
                } else {
                    throw new Error(`Error: ${response.status} - ${response.statusText}`);
                }
            })
                .then(data => {
                    console.log(data.message);
                    if (data.message === 'success') {
                        // Product can be added, show success message
                        swal.fire({
                            title: "Success",
                            text: "Product added to wishlist!",
                            icon: "success",
                            showConfirmButton: false  
                            });
                        setTimeout(() => {
                            window.location.reload()
                        }, 1000);
                    } else if (data.message === 'canAdd') {
                        // Product is already in the wishlist, show a different message
                        swal.fire("Info", "Product is already in the wishlist.", "info");
                    } else {
                        swal.fire({
                            title: "Info",
                            text: "Please Login to continue",
                            icon: "info",
                            
                        }).then(() => {
                            // Redirect to login page
                            window.location.href = '/login';
                        });

                    }
                    // window.location.reload();
                })
                .catch(error => {
                    console.log('Error adding product to wishlist', error);
                });

        }
    </script>
