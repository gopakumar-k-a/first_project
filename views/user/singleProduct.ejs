                                            window.location.href = `/add-to-cart?productId=${prId}&qty=${qty}&size=${size}`;



                                        } catch (error) {
                                            console.error('Error:', error);
                                        }
                                    }
                                    window.onload = function () {
                                        var sizeElement = document.getElementById('sizeSelect')
                                        selectedSize(sizeElement);
                                    };
                                </script>
