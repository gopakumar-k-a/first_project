            <footer class="main-footer font-xs">
                <div class="row pb-30 pt-15">
                    <div class="col-sm-6">
                        <script>
                            //categoryUpdate=================================
                            function validatecat() {
                                // Get the values of the form fields
                                const catName = document.getElementById('cat-name').value;

                                // Check if both fields are filled
                                if (catName.trim() === '') {
                                    // Display an error message
                                    document.getElementById('errMessage').innerText = 'All fields must be filled.';
                                    // Prevent form submission
                                    return false;
                                }

                                // Clear any previous error messages
                                document.getElementById('successMessage').innerText = '';

                                // Allow form submission
                                return true;
                            }
                            //update sweet alert
                            function launchSweetAlert(catId, name) {
                                Swal.fire({
                                    title: 'Update category name',
                                    html: `
                                        <form id="updateForm" action="/admin/edit-cat" method="POST">
                                            <input name="catName" type="text" id="inputField" class="swal2-input" value="${name || ''}">
                                            <input name="id" type="hidden" id="id" class="swal2-input" value="${catId || ''}">
                                        </form>
                                    `,
                                    icon: 'info',
                                    showCancelButton: true,
                                    confirmButtonText: 'Update Category Name',
                                    cancelButtonText: 'Cancel',
                                }).then((result) => {
                                    if (result.isConfirmed) {
                                        // Trigger form submission when the user clicks "Update Category Name"
                                        document.getElementById('updateForm').submit();
                                    }
                                });
                            }
                            // category block and unblock
                            function confirmBlock(categoryId) {
                                Swal.fire({
                                    title: 'Are you sure?',
                                    text: 'You are about to block this category.',
                                    icon: 'warning',
                                    showCancelButton: true,
                                    confirmButtonColor: '#d33',
                                    cancelButtonColor: '#3085d6',
                                    confirmButtonText: 'Yes, block it!'
                                }).then((result) => {
                                    if (result.isConfirmed) {
                                        window.location.href = "/admin/blockCategory?_id=" + categoryId;
                                    }
                                });
                            }

                            function confirmUnblock(categoryId) {
                                Swal.fire({
                                    title: 'Are you sure?',
                                    text: 'You are about to unblock this category.',
                                    icon: 'warning',
                                    showCancelButton: true,
                                    confirmButtonColor: '#28a745',
                                    cancelButtonColor: '#3085d6',
                                    confirmButtonText: 'Yes, unblock it!'
                                }).then((result) => {
                                    if (result.isConfirmed) {
                                        window.location.href = "/admin/unblockCategory?_id=" + categoryId;
                                    }
                                });
                            }



                            //========================update league data=========================================
                            //validate league

                            function validateLeague() {
                                // Get the values of the form fields
                                const leagueName = document.getElementById('league-name').value;

                                // Check if the field is filled
                                if (leagueName.trim() === '') {
                                    // Display an error message
                                    document.querySelector('.league-err').innerText = 'All fields must be filled.';
                                    // Prevent form submission
                                    return false;
                                }

                                // Clear any previous error messages
                                document.querySelector('.league-err').innerText = '';

                                // Allow form submission
                                return true;
                            }

                            function sweetAlertLeagueUpdate(leagueId, name) {
                                Swal.fire({
                                    title: 'Update category name',
                                    html: `
                                        <form id="updateLeagueForm" action="/admin/edit-league" method="POST">
                                            <input name="leagueName" type="text" id="inputField" class="swal2-input" value="${name || ''}">
                                            <input name="id" type="hidden" id="id" class="swal2-input" value="${leagueId || ''}">
                                        </form>
                                    `,
                                    icon: 'info',
                                    showCancelButton: true,
                                    confirmButtonText: 'Update Category Name',
                                    cancelButtonText: 'Cancel',
                                }).then((result) => {
                                    if (result.isConfirmed) {
                                        // Trigger form submission when the user clicks "Update Category Name"
                                        document.getElementById('updateLeagueForm').submit();
                                    }
                                });
                            }
                            //block league
                            function leagueBlock(leagueId) {
                                Swal.fire({
                                    title: 'Are you sure?',
                                    text: 'You are about to block this category.',
                                    icon: 'warning',
                                    showCancelButton: true,
                                    confirmButtonColor: '#d33',
                                    cancelButtonColor: '#3085d6',
                                    confirmButtonText: 'Yes, block it!'
                                }).then((result) => {
                                    if (result.isConfirmed) {
                                        window.location.href = "/admin/blockLeague?_id=" + leagueId;
                                    }
                                });
                            }
                            //unblock league
                            function leagueUnblock(leagueId) {
                                Swal.fire({
                                    title: 'Are you sure?',
                                    text: 'You are about to unblock this category.',
                                    icon: 'warning',
                                    showCancelButton: true,
                                    confirmButtonColor: '#28a745',
                                    cancelButtonColor: '#3085d6',
                                    confirmButtonText: 'Yes, unblock it!'
                                }).then((result) => {
                                    if (result.isConfirmed) {
                                        window.location.href = "/admin/unblockLeague?_id=" + leagueId;
                                    }
                                });
                            }
                            //=======================team management=================================

                            function sweetAlertTeamUpdate(teamId, name) {
                                Swal.fire({
                                    title: 'Update Team name',
                                    html: `
                                        <form id="updateTeamForm" action="/admin/edit-team" method="POST">
                                            <input name="teamName" type="text" id="inputField" class="swal2-input" value="${name || ''}">
                                            <input name="id" type="hidden" id="id" class="swal2-input" value="${teamId || ''}">
                                        </form>
                                    `,
                                    icon: 'info',
                                    showCancelButton: true,
                                    confirmButtonText: 'Update Category Name',
