            <!-- ========================================category================================== -->
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Categories </h2>
                    <p>Add, edit or delete a category</p>
                </div>
              
            </div>
            <div class="card">
                <div class="card-body">
                    <div class="row">
                        <div class="col-md-3">
                            <form action="/admin/add-category" method="post" onsubmit="return validatecat()">
                                <div class="mb-4">
                                    <label for="product_name" class="form-label">Category Name</label>
                                    <input type="text" placeholder="Type here" class="form-control" id="cat-name"
                                        name="categoryName">

                                </div>

                                <span class="text-center text-success">
                                    <%= csMessage %>
                                </span>
                                
                                <span class="text-center text-danger">
                                    <%= ceMessage %>
                                </span>
                                <span class="text-center text-danger" id="errMessage">

                                </span>
                                <div class="d-grid">
                                    <button type="submit" class="btn btn-primary">Create category</button>

                                </div>
                            </form>
                        </div>
                        <div class="col-md-9">
                            <div class="container" style="height: 400px; overflow-y: scroll;">
                                <div class="table-responsive">
                                    <table class="table table-hover">
                                        <thead>
                                            <tr>
                                                <th>Sl No</th>
                                                <th>Name</th>
                                                <th>Status</th>
                                                <th>block/unblock</th>
                                                <th> </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% for(let i=0;i<catData.length;i++){ %>
                                                <tr>
                                                    <td>
                                                        <%=i+1 %>
                                                    </td>
                                                    <td><b>
                                                            <%= catData[i].name %>
                                                        </b></td>
                                                    <td>
                                                        <% if (catData[i].isActive==true) { %>
                                                            <div class="col-lg-2 col-sm-2 col-4 col-status">
                                                                <span class="badge rounded-pill alert-success"
                                                                    onclick="confirmStatusChange('<%= catData[i]._id %>')">Active</span>
                                                            </div>
                                                            <% } else { %>
                                                                <div class="col-lg-2 col-sm-2 col-4 col-status">
                                                                    <span
                                                                        class="badge rounded-pill alert-danger">Disabled</span>
                                                                </div>
                                                                <% } %>
                                                    </td>
                                                    <td>
                                                        <% if (catData[i].isActive===true) { %>
                                                            <a href="#" onclick="confirmBlock('<%= catData[i]._id %>')">
                                                                <button
                                                                    class="btn btn-sm btn-danger rounded font-sm">Block</button>
                                                            </a>
                                                            <% } else { %>
                                                                <a href="#"
                                                                    onclick="confirmUnblock('<%= catData[i]._id %>')">
                                                                    <button
                                                                        class="btn btn-sm btn-success rounded font-sm ml-2">Unblock</button>
                                                                </a>
                                                                <% } %>
                                                    </td>
                                                    <td>
                                                        <button type="button" class="btn btn-primary"
                                                            onclick="launchSweetAlert('<%= catData[i]._id %>','<%= catData[i].name %>')">
                                                            edit
                                                        </button>
                                                    </td>
                                                </tr>
                                                <% } %>
                                        </tbody>
                                    </table>
                                </div>
                            </div>

                        </div> <!-- .col// -->
                    </div> <!-- .row // -->
                </div> <!-- card body .// -->
            </div> <!-- card .// -->
            </section> <!-- content-main end// -->


            </div>


