                                                    id="limitOfCoupon<%=index%>" name="limitOfCoupon<%=index%>" value="<%=coupon.limit%>">
                                                <h6 class="text-danger" id="limitOfCouponErr<%=index%>"> </h6>

                                            </div>
                                            <div class="row">
                                                <div class="col-lg-4">
